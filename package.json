{
  "private": true,
  "name": "gabrielcaiana.com",
  "author": "Gabriel Caiana",
  "version": "0.0.0",
  "description": "This is the new version of my site using nuxt 3",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gabrielcaiana/gabrielcaiana.com-nuxt3"
  },
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "husky install",
    "lint": "eslint --ext \".ts,.js,.vue\" --ignore-path .gitignore .",
    "lintfix": "eslint --fix --ext \".ts,.js,.vue\" --ignore-path .gitignore .",
    "prepare": "husky install"
  },
  "devDependencies": {
    "nuxt": "3.0.0-rc.1",
    "@nuxtjs/eslint-config-typescript": "^7.0.2",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-nuxt": "^3.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.0",
    "lint-staged": "^12.3.3"
  },
  "lint-staged": {
    "**/*.{js,ts,vue,html,css,scss}": [
      "yarn lintfix"
    ]
  }
}
